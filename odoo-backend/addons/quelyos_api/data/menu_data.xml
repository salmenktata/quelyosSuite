            <field name="url">/cart</field>
            <field name="parent_id" ref="menu_footer_quick"/>
            <field name="sequence">20</field>
        </record>

        <record id="menu_footer_quick_wishlist" model="quelyos.menu">
            <field name="name">Liste de souhaits</field>
            <field name="code">footer_quick_wishlist</field>
            <field name="label">Liste de souhaits</field>
            <field name="url">/wishlist</field>
            <field name="parent_id" ref="menu_footer_quick"/>
            <field name="sequence">30</field>
        </record>

        <record id="menu_footer_quick_compare" model="quelyos.menu">
            <field name="name">Comparateur</field>
            <field name="code">footer_quick_compare</field>
            <field name="label">Comparateur</field>
            <field name="url">/compare</field>
            <field name="parent_id" ref="menu_footer_quick"/>
            <field name="sequence">40</field>
        </record>

        <!-- Menu Footer Service Client -->
        <record id="menu_footer_service" model="quelyos.menu">
            <field name="name">Footer - Service Client</field>
            <field name="code">footer_service</field>
            <field name="label">Service Client</field>
            <field name="url">#</field>
            <field name="sequence">3</field>
            <field name="active" eval="True"/>
        </record>

        <record id="menu_footer_service_shipping" model="quelyos.menu">
            <field name="name">Livraison</field>
            <field name="code">footer_service_shipping</field>
