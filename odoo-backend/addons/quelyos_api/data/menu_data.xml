            <field name="parent_id" ref="menu_footer_service"/>
            <field name="sequence">30</field>
        </record>

        <record id="menu_footer_service_cgv" model="quelyos.menu">
            <field name="name">CGV</field>
            <field name="code">footer_service_cgv</field>
            <field name="label">Conditions Générales</field>
            <field name="url">/cgv</field>
            <field name="parent_id" ref="menu_footer_service"/>
            <field name="sequence">40</field>
        </record>
    </data>
</odoo>
